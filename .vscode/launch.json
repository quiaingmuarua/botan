{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug AES Example",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/aes_example",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "DYLD_LIBRARY_PATH",
                    "value": "${workspaceFolder}"
                }
            ],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for lldb",
                    "text": "settings set target.prefer-dynamic-value run-target",
                    "ignoreFailures": true
                }
            ],
